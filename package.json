{
  "name": "vuejs-toggle-switch",
  "description": "A toggle switch component for Vue.js",
  "version": "1.3.3",
  "author": "LMH, lars@softwarefun.no , softwarefun.no",
  "license": "MIT",
  "homepage": "https://github.com/larsmars/vuejs-toggle-switch#readme",
  "private": false,
  "main": "./dist/ToggleSwitch.common.js",
  "scripts": {
    "lint": "vue-cli-service lint",
    "serve": "vue-cli-service serve",
    "build-bundle": "vue-cli-service build --target lib --name ToggleSwitch ./src/components/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/larsmars/vuejs-toggle-switch.git"
  },
  "bugs": {
    "url": "https://github.com/larsmars/vuejs-toggle-switch/issues"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.12",
    "@vue/cli-plugin-eslint": "^4.5.12",
    "@vue/cli-service": "^4.5.6",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.2",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.3",
    "node-sass": "^4.14.1",
    "sass-loader": "^7.0.1",
    "vue": "^2.6.12",
    "vue-template-compiler": "^2.6.12"
  },
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "eslintConfig": {
    "root": true,
    "env": {
